<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="DoRegistration">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload 2 Point Cloud Datasets for the registration. (extension for .las,.laz,.ply only.)</p>
            <input type="file" name="files" accept=".las,.laz,.ply" multiple/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>