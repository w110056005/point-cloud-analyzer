<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="DoStitch">
    <div class="form-group">
        <div>
            <p>Upload 5 Point Cloud Datasets for the Stitch. (extension for .las,.laz,.ply only.)</p>
        </div>
        <div>
            Data 1: <input type="file" name="file1" accept=".las,.laz,.ply" />
        </div>   
        <div>
            Data 2: <input type="file" name="file2" accept=".las,.laz,.ply" />
        </div>  
        <div>
            Data 3: <input type="file" name="file3" accept=".las,.laz,.ply" />
        </div>   
        <div>
            Data 4: <input type="file" name="file4" accept=".las,.laz,.ply" />
        </div>   
        <div>
            Data 5: <input type="file" name="file5" accept=".las,.laz,.ply" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>